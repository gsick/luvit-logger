[
  {
    "name": "redis_logger",
    "level": "trace",
    "loggers": [
      {
        "type": "redis",
        "port": 6379,
        "host": "127.0.0.1",
        "cmd":"rpush",
        "key": "log",
        "level": "info",
        "date": false,
        "reconnect": true
      },
      {
        "type": "redis",
        "port": 6379,
        "host": "127.0.0.1",
        "cmd":"publish",
        "key": "log_channel",
        "level": "info",
        "reconnect": true
      },
      {
        "type": "file",
        "path": "/tmp/log-redis.log"
      }
    ]
  }
]

